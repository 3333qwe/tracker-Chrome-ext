{
  "name": "Procrastinationation",
  "description": "Don't surf around, you will be tracedf!",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Procrastinationation tracker"
  },

  "manifest_version": 2,
  "chrome_url_overrides" : {
        "newtab": "newtab.html"
  }

}
